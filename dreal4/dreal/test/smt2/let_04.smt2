(set-logic QF_NRA)
(declare-const a Real)
(declare-const b Real)
(assert (= a -5))
(assert (= b 7))
(assert (and (let ((a (* b 10)) (b (* a 10)))
               (and (= a 70) (= b -50)
                 (let ((a (+ a 30)) (c (- a 70)))
                   (and (= a 100) (= b -50) (= c 0)))))
             (< a -1) (> b 1)))
(check-sat)
(exit)
